{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
      "functions": {
        "api/**/*.js": {
          "includeFiles": "node_modules/tesseract.js-core/tesseract-core-simd.wasm"
        }
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}